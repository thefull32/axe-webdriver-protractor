{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "declaration": false,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "exclude": [
      "../node_modules",
        "plugins.ts"
    ]
}